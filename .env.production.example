# Production Environment Variables
# Copy this to .env.production on your EC2 instance and fill in real values

# Database Configuration
DB_URL=jdbc:postgresql://your-db-host:5432/your-db-name?sslmode=require
DB_USERNAME=your-db-username
DB_PASSWORD=your-db-password

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-min-256-bits-change-this-in-production
JWT_ACCESS_TOKEN_EXPIRATION=900000
JWT_REFRESH_TOKEN_EXPIRATION=604800000

# Application Configuration
JPA_SHOW_SQL=false
